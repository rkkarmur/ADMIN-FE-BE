
generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}


model User {
    id     String @id @default(auto()) @map("_id") @db.ObjectId
    name   String
    email  String @unique
    roleId String
    role   Role   @relation(fields: [roleId], references: [id])
}


model Role {
    id    String @id @default(auto()) @map("_id") @db.ObjectId
    name  String
    users User[]
}
